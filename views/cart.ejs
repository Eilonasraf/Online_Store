<!DOCTYPE html>
<html lang="en">
<head>
  <!-- Your head elements -->
</head>
<body>
  <h1>Your Cart</h1>
  <% if (cart.products.length > 0) { %>
    <% cart.products.forEach((product) => { %>
      <div>
        <h2><%= product.name %></h2>
        <p>$<%= product.price %></p>
        <!-- Add a remove from cart button here if needed -->
      </div>
    <% }) %>
    <form action="/checkout" method="post">
        <button type="submit">Proceed to Checkout</button>
      </form>
  <% } else { %>
    <p>Your cart is empty.</p>
  <% } %>
</body>
</html>
